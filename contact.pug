extends base.pug 

block scripts  
    script
      include ../static/index.js
      
block style 
    style
      include ../static/style.css
      include ../static/styleContact.css

block content   
    div.container
        h1 Contact us
        form(action="/contact" method="post" class="MyForm") 
            input(type="text" name="name" class="MyInput" placeholder="Enter Your Name")
            input(type="number" name="age" class="MyInput" placeholder="Enter Your Age")
            input(type="number" name="phone" class="MyInput" placeholder="Enter Your Phone Number")
            input(type="email" name="email" class="MyInput" placeholder="Enter Your Email ID")
            input(type="text" name="desc" class="MyInput" placeholder="Enter Your Quetion")
            button.btn 
                | Submit